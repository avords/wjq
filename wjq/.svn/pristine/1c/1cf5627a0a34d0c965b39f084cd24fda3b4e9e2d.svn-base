#cookie save path
cookiePath = /
#domain
cookieDomain = localhost
cookieWebDomain=localhost
#root url
rootUrl=
#login base  url
loginUrl = /wjq/home/login
#portal application name
portalServerName = wjq
#permission refresh time
refreshPermissionTime = 30
#session id prefix
sessionName = sid
#validation code
validateCode = 123
#to enable single login for account
singleLogin=false

# the waiting time after error login
loginErrorWaitTime=5

#Access control for all URL:deny|allow, default is allow
accessControlAll=allow