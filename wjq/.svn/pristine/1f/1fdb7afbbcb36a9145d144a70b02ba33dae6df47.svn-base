<decorators defaultdir="/">
	<excludes>
		<pattern>*ajax=1*</pattern>
		<pattern>/login/*</pattern>
		<pattern>/decorator</pattern>
		<pattern>/homeDecorator</pattern>
	</excludes>
	<decorator name="decorator" page="decorator">
        <pattern>/*</pattern>
        <pattern>/admin*</pattern>
    </decorator>
    <decorator name="homeDecorator" page="homeDecorator">
		<pattern>/home/*</pattern>
		<pattern>/member/*</pattern>
	</decorator>
</decorators>